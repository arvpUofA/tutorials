        <uri>model://hokuyo_laser</uri>
        <pose>0.0 0 0.3 0 -0 0</pose>
    </include>
     <joint name='laser_joint' type='fixed'>
        <pose frame=''>0 0 0 0 -0 0</pose>
        <child>hokuyo_laser::hokuyo_link</child>
        <parent>chassis</parent>

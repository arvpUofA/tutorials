    <child>left_wheel</child>
    <parent>chassis</parent>
    <axis>
        <xyz>0 1 0</xyz>
        <limit>
        <lower>-1e+16</lower>
        <upper>1e+16</upper>
        </limit>
    </axis>
    </joint>
    <joint name='right_wheel_hinge' type='revolute'>
